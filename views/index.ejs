<%- include header %>
<!-- Header -->
<header>
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <img class="img-responsive" src="images/common/YSP.png" alt="">
                <div class="intro-text">
                    <span class="name">加班工具网</span>
                    <span class="skills">More Tools, More Work</span>
                </div>
            </div>
        </div>
    </div>
</header>
<!-- About Section -->
<section class="success" id="about">
    <% posts.forEach(function(post, index) { %>
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2><%= post.title %></h2>
                    <hr class="star-light">
                </div>
            </div>
            <div class="row">
                <div class="col-lg-7 col-lg-offset-2">
                    <p><%= post.contents %></p>
                    <p><a href="/inputDrag">inputDrag</a>
                </div>
            </div>
        </div>
    <% }) %>
</section>
<%- include footer %>